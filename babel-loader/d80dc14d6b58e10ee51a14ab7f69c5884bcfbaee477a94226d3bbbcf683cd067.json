{ name: item.name })\r\n        },\r\n        //点击tag删除功能\r\n        handleClose(item, index) {\r\n            //调用store中的mutation\r\n            this.closeTag(item)\r\n            const length = this.tags.length\r\n            //删除之后的跳转逻辑\r\n            if (item.name !== this.$route.name) {\r\n                return\r\n            }\r\n            //表示的是删除的最后一项\r\n            if (index === length) {\r\n                this.$router.push({\r\n                    name: this.tags[index - 1].name\r\n                })\r\n            } else {\r\n                this.$router.push({\r\n                    name: this.tags[index].name\r\n                })\r\n            }\r\n        }\r\n    }\r\n\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.tabs {\r\n    padding: 20px;\r\n\r\n    .el-tag {\r\n        margin-right: 15px;\r\n        cursor: pointer;\r\n    }\r\n}\r\n</style>>\r\n\r\n"],"mappings":";AAWA,SAAAA,QAAA,EAAAC,YAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAJ,QAAA;MACAK,IAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAC,GAAA,CAAAC;IACA;EACA;EAAAC,OAAA;IACA,GAAAR,YAAA;IACA;IACAS,WAAAC,IAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAX,IAAA,EAAAS,IAAA,CAAAT;MAAA;IACA;IACA;IACAY,YAAAH,IAAA,EAAAI,KAAA;MACA;MACA,KAAAC,QAAA,CAAAL,IAAA;MACA,MAAAM,MAAA,QAAAZ,IAAA,CAAAY,MAAA;MACA;MACA,IAAAN,IAAA,CAAAT,IAAA,UAAAgB,MAAA,CAAAhB,IAAA;QACA;MACA;MACA;MACA,IAAAa,KAAA,KAAAE,MAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;UACAX,IAAA,OAAAG,IAAA,CAAAU,KAAA,MAAAb;QACA;MACA;QACA,KAAAU,OAAA,CAAAC,IAAA;UACAX,IAAA,OAAAG,IAAA,CAAAU,KAAA,EAAAb;QACA;MACA;IACA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}